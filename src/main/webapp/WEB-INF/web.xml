<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>view.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>company-sign-up</servlet-name>
    <servlet-class>view.CompanySignUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>passenger-sign-up</servlet-name>
    <servlet-class>view.PassengerSignUp</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>company-menu</servlet-name>
    <servlet-class>view.CompanyMenu</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>passenger-menu</servlet-name>
    <servlet-class>view.PassengerMenu</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>passenger-menu</servlet-name>
    <url-pattern>/passengermenu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>company-menu</servlet-name>
    <url-pattern>/companymenu</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>passenger-sign-up</servlet-name>
    <url-pattern>/passengersignup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>company-sign-up</servlet-name>
    <url-pattern>/companysignup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>
